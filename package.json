{
  "name": "readr",
  "version": "0.8.4",
  "private": true,
  "scripts": {
    "start": "npm run server:prod",
    "test": "npm run mocha --silent",
    "test:ts": "mocha --compilers ts:ts-node/register,tsx:ts-node/register",
    "mocha": "mocha --require babel-register",
    "server": "npm run server:fe:hot",
    "server:fe:file": "npm run watch:css & npm run watch:chunks & npm run server:dev",
    "server:fe:hot": "npm run watch:css & node bin/server --hot --noRoute",
    "server:fe": "npm run watch:css & npm run watch:chunks & node bin/server --noRoute",
    "server:dev": "nodemon --watch bin/server bin/server",
    "server:dev:watch": "npm run watch & nodemon --watch bin/server bin/server",
    "server:prod": "nodemon --watch bin/server bin/server --production",
    "watch": "npm run watch:node",
    "watch:fe": "npm run watch:css & npm run watch:chunks",
    "watch:fe:prod": "npm run watch:css:prod & npm run watch:chunks:prod",
    "watch:node": "npm run build:node -- --watch",
    "watch:css": "npm run build:css -- --watch",
    "watch:css:prod": "npm run build:css:prod -- --watch",
    "watch:chunks:prod": "npm run build:chunks:prod -- --watch",
    "watch:chunks": "npm run build:chunks -- --watch",
    "build": "npm run build:css:prod && npm run build:node && npm run build:chunks:prod",
    "build:node": "webpack --progress --config ./webpack/node.config.js",
    "build:css": "webpack --progress --config ./webpack/css.config.js",
    "build:css:prod": "webpack --progress --config ./webpack/css.prod.config.js",
    "build:dll": "webpack --progress --config ./webpack/dll.config.js",
    "build:chunks:prod": "webpack --progress --config ./webpack/chunks.prod.config.js",
    "build:chunks": "webpack --progress --config ./webpack/chunks.config.js",
    "build:es6": "tsc",
    "clean:all": "rimraf -- bin public/built public/built_prod __production__",
    "clean:bin": "rimraf -- bin",
    "clean:built-dev": "rimraf -- public/built",
    "clean:built-prod": "rimraf -- public/built_prod",
    "clean:deploy": "rimraf -- __production__",
    "deploy": "gulp copy",
    "predeploy": "npm run clean:built-prod && npm run clean:deploy && npm run build",
    "preupdate-own-pack": "npm config set registry https://registry.npmjs.org/",
    "update-own-pack": "npm update simple-epub-parser better-redux-form --save",
    "postupdate-own-pack": "npm config set registry https://registry.npm.taobao.org"
  },
