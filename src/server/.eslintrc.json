{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [2, "never"],
    "strict": [0],
    "handle-callback-err": [2],
    "arrow-body-style": [0],
    "prefer-const": [1],
    "prefer-rest-params": [0],
    "no-param-reassign": [0],
    "max-len": [1, 200],
    "no-console": [1, { "allow": ["warn", "error"] }],
    "no-underscore-dangle": [0],
    "no-trailing-spaces": [0],
    "comma-dangle": [0],
    "no-shadow": [1],
    "no-unused-vars": [1],
    "prefer-template": [0],
    "global-require": [0]
  }
}