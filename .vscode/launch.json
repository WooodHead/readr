  "version": "0.1.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Serve pages in production",
      "program": "${workspaceRoot}/bin/index.js",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "args": [
        "--serve=pages",
        "--production"
      ],
      "outFiles": [
        "${workspaceRoot}/build/node/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Serve pages in development",
      "program": "${workspaceRoot}/bin/index.js",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "args": [
        "--serve=pages"
      ],
      "outFiles": [
        "${workspaceRoot}/build/node/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Serve api in development",
      "program": "${workspaceRoot}/bin/index.js",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "args": [
        "--serve=api"
      ],
      "outFiles": [
        "${workspaceRoot}/build/node/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "附加到进程",
      "port": 5858
    }
  ]
}
